<!-- Wine Picture -->
<div id="container">

  <video id="background_video" loop muted autoplay>
    <source src="/assets/WINE_POUR_SLOW_MOTION.mp4" type="video/mp4">
  </video>
</div>


<!-- <div id="video_cover"></div> -->
<div id="overlay"></div>

<!-- Title and signup/login buttons -->
<section id="main_content">
  <div id="head">
    <h1>Omniscient Aerator: Sign Up</h1>
  </div>

<div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input id="SUuser_name" type="text" class="validate">
          <label for="user_name">User Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="SUemail" type="email" class="validate">
          <label for="email">Email</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="SUpassword" type="password" class="validate">
          <label for="password">Password</label>
        </div>
      </div>
    </form>
  </div>

  <div id="links">
    <!-- Sign Up button -->
  <input class="btn btn-primary" type="submit" value="Sign Up"/>
    


  </div>
</section>








